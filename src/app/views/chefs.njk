{% extends "layout.njk" %}

{% block content %}
  <div class="container">

    <div class="listaChefeAdm">
      
      {% for chef in chefs %}
          
          <img class="avatarChefe" src="{{ chef.img }}" />

          <div class="dadosInline">
            <div class="nomeChefeAdm">{{ chef.name }}</div>
            <div class="autorDetalhe">
              {{ chef.qtde_receitas }} receita(s)
            </div>
          </div>

        {% endfor %}
    <div>
  </div>
{% endblock content %}